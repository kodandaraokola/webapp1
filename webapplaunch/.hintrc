{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "@-ms-viewport",
          "@-ms-viewport",
          "-webkit-overflow-scrolling",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not firefox <= 113",
    "not safari <= 16.5",
    "not samsung <= 21"
  ]
}